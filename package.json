    "@loaders.gl/core": "^3.4.13",
    "@loaders.gl/obj": "^3.4.13",
    "@reduxjs/toolkit": "^1.9.5",
    "chayns-api": "^1.0.20",
    "chayns-components": "^4.31.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "tobit-websocket-service-client": "^1.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.33",
    "chayns-toolkit": "^2.0.8",
    "prettier": "^2.8.8",
